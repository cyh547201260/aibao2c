/**index.wxss**/
page{
  background: #F1F1F1;
}
.filter-box-fixed{
  position: fixed;
  width: 100%;
  left: 0;
  top: 120rpx;
  margin-top: 0 !important;
}
.search-show-header{
  background: white !important;
  border-bottom: 1rpx solid #EEEEEE;
}
.search-show-header input{
  width: 75% !important;
  margin: 0 !important;
  margin-left: 30rpx !important;
  display: inline-block;
  background: #F7F7F7 !important;
}
.filter-box-fixed-tonone{
  display: block !important;
}
.filter-list-box-none{
  display: none !important;
}
.search-top-box{
  background: #4AD59E;
  padding: 20rpx 0;
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  z-index: 99999;
}
.search-top-box input{
  width: 90%;
  height: 80rpx;
  border-radius: 44rpx;
  margin: 0 auto;
  font-size: 32rpx;
  background: white;
  padding-left: 42rpx;
  transition: width .1s;
}
.banner-slide{
  background: #FFDB2F;
  margin-top: 120rpx;
}
.banner-slide .swiper-item view{
  width: 100%;
  height: 100%;
  background: #14B65B;
}
.nav-box{
  overflow: hidden;
  background: white;
}
.nav-box .nav-item{
  float: left;
  width: 20%;
  height: 200rpx;
  text-align: center;
}
.nav-box .nav-item image{
  width: 100rpx;
  height: 100rpx;
  background: #FF6230;
  border-radius: 50%;
  margin: 0 auto;
  margin-top: 31rpx;
}
.nav-box .nav-item span{
  font-size: 22rpx;
  text-align: center;
  color: #666;
  display: block;
}
.filter-list-box{
  background: white;
  height: 73rpx;
  text-align: center;
  line-height: 73rpx;
  margin-top: 20rpx;
  border-bottom: 1rpx solid #DDDDDD;
  z-index: 99999;
}
.list-item-box{
  display: flex;
}
.list-item-box .filter-list-item{
  flex: 1;
  color: #333;
  font-size: 28rpx;
  position: relative;
  background: white;
}
.filter-list-box .filter-list-item::after{
  content: " ";
  position: absolute;
  top: 19rpx;
  bottom: 19rpx;
  right: 0;
  width: 1rpx;
  border-right: 1rpx solid #D8D8D8;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleX(.5);
  transform: scaleX(.5);
}
.data-list{
  position: relative;
}
.filter-btn-list-box{
  width: 100%;
  background: white;
  display: none;
  position: relative;
  z-index: 99999;
}
.filter-btns-box view{
  font-size: 26rpx;
  color: #333333;
  height: 92rpx;
  line-height: 92rpx;
  padding-left: 30rpx;
  position: relative;
  text-align: left;
}
.filter-btns-box view:first-child::before{
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1rpx;
  border-top: 1rpx solid #D8D8D8;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
}
.filter-btns-box view::after{
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1rpx;
  border-bottom: 1rpx solid #D8D8D8;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
}
.filter-mask-back{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background: black;
  opacity: .5;
  z-index: 999;
  display: none;
}
.filter-mask-show{
  display: block !important;
}
.data-list .data-list-item{
  background: white;
  position: relative;
  padding: 0 25rpx;
}
.data-list .data-list-item::after{
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1rpx;
  border-bottom: 1rpx solid #D8D8D8;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
}
.item-top{
  display: flex;
  position: relative;
}
.item-top-img{
  flex: 36;
  width: 220rpx;
  height: 140rpx;
  background: #d8d8d8;
  margin: 25rpx 25rpx 25rpx 0;
}
.item-top-info{
  flex: 64;
}
.item-top::after{
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1rpx;
  border-bottom: 1rpx solid #dddddd;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
}
.item-top-info .top-info-name{
  font-size: 30rpx;
  color: #333;
  margin-top: 33rpx;
}
.top-info-classname{
  display: flex;
}
.top-info-classname span:first-child{
  flex: 6;
  text-align: left;
  font-size: 26rpx;
  color: #666;
}
.top-info-classname span:nth-child(2){
  flex: 4;
  text-align: right;
  color: #333;
  font-size: 26rpx;
}
.item-bot{
  padding: 23rpx 0;
}
.item-bot .red-tip{
  font-size: 22rpx;
  color: white;
  background: #FF4242;
  border-radius: 19rpx;
  height: 36rpx;
  width: 120rpx;
  display: inline-block;
  text-align: center;
  line-height: 36rpx;
  transform: translateY(-3rpx);
}
.item-bot .tip-cont{
  font-size: 26rpx;
  color: #999;
  margin-left: 15rpx;
  line-height: 36rpx;
}
.filter-full-box{
  height: 80vh;
}
.filter-step2-box{
  position: absolute;
  width: 50%;
  height: 100%;
  right: 0;
  background: #F1F1F1;
  top: 0;
}
.filter-step2-box.filter-address-left{
  left: 0;
  background: white;
}
.filter-step-box-scroll view{
  font-size: 26rpx;
  color: #333333;
  height: 92rpx;
  line-height: 92rpx;
  padding-left: 30rpx;
  position: relative;
  text-align: left;
}
.filter-step-box-scroll view:first-child::before{
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1rpx;
  border-top: 1rpx solid #D8D8D8;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
}
.filter-step-box-scroll view::after{
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1rpx;
  border-bottom: 1rpx solid #D8D8D8;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
}
.classStap_act{
  background: #F1F1F1;
  color: #4AD59E !important;
}
.search-body-box{
  position: fixed;
  width: 100%;
  height: 100%;
  background: white;
  left: 0;
  top: 0;
  z-index: 99998;
}
.search-btn{
  position: absolute;
  text-align:center;
  width:15%;
  line-height:80rpx;
  font-size: 30rpx;
  color: #666;
  top:20rpx;
  right: 0;
}
.search-body-title{
  font-size: 28rpx;
  color: #333;
  padding: 15rpx 30rpx;
}
.search-body-box{
  padding-top: 135rpx;
}
.search-cont-list-box{
  overflow: hidden;
  padding: 0 30rpx;
}
.search-cont-list-box .search-cont-item{
  float: left;
  font-size: 24rpx;
  color: #333;
  height: 40rpx;
  line-height: 40rpx;
  padding: 5rpx 20rpx;
  background: #f1f1f1;
  border-radius: 20rpx;
  margin:20rpx 35rpx 20rpx 0;
}
.clear-search-btn{
  font-size: 26rpx;
  color: #333;
  text-align: center;
  position: absolute;
  width: 100%;
  bottom: 50%;
  padding: 10rpx;
}
.clear-search-btn image{
  width: 24rpx;
  height: 25rpx;
  transform: translateY(2rpx); 
}